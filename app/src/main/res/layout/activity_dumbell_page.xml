<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".dumbell_page">

    <ImageView
            android:id="@+id/imageview"
            android:layout_width="92dp"
            android:layout_height="69dp"
            android:background="@drawable/black_white_elegant_monogram_initial_name_logo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:id="@+id/image_view"
            android:layout_width="355dp"
            android:layout_height="37dp"
            android:background="@drawable/to"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.099"/>

    <LinearLayout
            android:layout_width="402dp"
            android:layout_height="483dp"
            android:orientation="vertical"
            android:padding="16dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.536" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.508" android:id="@+id/linearLayout">

        <Button
                android:id="@+id/bicep_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                android:text="Bicep Curls"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
        />
        <Button
                android:id="@+id/lateral_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
                android:text="Lateral raise"
        />
        <Button
                android:id="@+id/sitting_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                android:text="Shoulder shrug"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
        />
        <Button
                android:id="@+id/inclinebench_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
                android:text="Incline Benh Press"
        />
        <Button
                android:id="@+id/cablepull_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                android:text="Cable Pull"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
        />
        <Button
                android:id="@+id/benchpress_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
                android:text="Bench Press"
        />
        <Button
                android:id="@+id/cablerow_btn"
                android:layout_width="333dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                app:icon="@drawable/_5d92c6e_a92e_4e17_8491_a6d87ccd2db0_12"
                android:text="Cable Row"
        />
    </LinearLayout>

    <Button
            android:id="@+id/start_btn"
            android:text="Start"
            android:layout_width="229dp"
            android:layout_height="40dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.929"
            app:layout_constraintEnd_toEndOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>